<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="partner_connect" model="ir.ui.view">
         <field name="name">connect.partner</field>
            <field name="model">connect.partner</field>
            <field name="arch" type="xml">
                <search string="Search active Items">
                     <filter string="partner_connect" name="active" domain="[('active', '=', True)]"/>
                </search>
            </field>
    </record>
    <record id="view_connect_partner_form" model="ir.ui.view">
        <field name="name">connect.partner</field>
        <field name="model">connect.partner</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="subscription_type" widget="many2many_tags"/>
                            <field name="partner_id"/>
                        </group>
                        <group>
                            <field name="active"/>
                        </group>
                    </group>
                    <group>
                        <notebook colspan="4">
                            <page string="Information" name="information">
                                <group>
                                     <field name="phone_number"/>
                                </group>
                            </page>
                        </notebook>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
     <record id="view_connect_partner_type_form" model="ir.ui.view">
        <field name="name">connect.partner.subscription.type</field>
        <field name="model">connect.partner.subscription.type</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                        </group>
                        <group>

                        </group>
                    </group>
                                <notebook colspan="4">
                                    <page string="Information" name="information">
                                        <group>
                                             <field name="product_ids"/>
                                        </group>
                                    </page>
                                </notebook>

                </sheet>
            </form>
        </field>
    </record>
    <record id="connect_subscription_product_view_form" model="ir.ui.view">
        <field name="name">connect.subscription.product</field>
        <field name="model">connect.subscription.product</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                            <group>
                                <field name="name"/>
                                <field name="price"/>
                                <field name="amount"/>
                            </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>