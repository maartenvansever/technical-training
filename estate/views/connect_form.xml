<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_connect_partner_form" model="ir.ui.view">
        <field name="name">connect.partner</field>
        <field name="model">connect.partner</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="active"/>
                        </group>
                    </group>
                    <group>
                        <notebook colspan="4">
                            <page string="Information" name="information">
                                <group>
                                     <field name="phone_number"/>
                                </group>
                            </page>
                        </notebook>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>